{
  "name": "nano-router",
  "version": "1.0.0",
  "private": true,
  "description": "A tiny modern router",
  "main": "./lib/index.js",
  "scripts": {
    "test": "lerna run test",
    "build": "lerna run build",
    "format": "prettier --write '**/*.{js,md,json}'",
    "update-toc": "for f in `ls packages/*/*.md`; do markdown-toc -i $f; done; npm run format",
    "lint": "eslint . && prettier --check '**/*.{js,md,json}'",
    "ci": "npm run lint && npm test",
    "release": "node-publisher release"
  },
  "keywords": [
    "React",
    "router",
    "simple"
  ],
  "author": "Sune Simonsen",
  "license": "MIT",
  "bugs": "https://github.com/sunesimonsen/nano-router/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/sunesimonsen/nano-router"
  },
  "devDependencies": {
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-mocha": "^10.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "eslint-plugin-standard": "^4.0.1",
    "lerna": "^4.0.0",
    "markdown-toc": "^1.2.0",
    "node-publisher": "^1.5.2",
    "prettier": "^2.1.0"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "engines": {
    "node": ">=16"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
